package Libraries.Development.Environment.Studio.Behaviors

use Libraries.Interface.Events.BehaviorEvent
use Libraries.Development.Environment.Studio.Interface.VisualEditor.NewItemPreview3D

class NewItemCycleModelBehavior is QuorumStudioBehavior
    action Run(BehaviorEvent event)
        if event:GetItem() is NewItemPreview3D
            NewItemPreview3D preview = cast(NewItemPreview3D, event:GetItem())
            preview:CyclePresetModel()
        end
    end
end