package Libraries.Data.Formats
use Libraries.Containers.HashTable

class JavaScriptObjectNotationObject 
    HashTable<text, JavaScriptObjectNotationObject> hash

    action Add(text key, JavaScriptObjectNotationObject object)
        hash:Add(key, object)
    end

    action Remove(text key)
        hash:RemoveAllKeys(key)
    end

    action GetSize returns integer
        return hash:GetSize()
    end
end