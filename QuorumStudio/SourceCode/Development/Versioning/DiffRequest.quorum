package Libraries.Development.Versioning
use Libraries.System.File

class DiffRequest
    File file = undefined
    text currentText = undefined

    action GetFile returns File
        return file
    end

    action SetFile(File file)
        me:file = file
    end
    action GetCurrentText returns text
        return currentText
    end

    action SetCurrentText(text currentText)
        me:currentText = currentText
    end

end