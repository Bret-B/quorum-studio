package Libraries.Language.Debug

class VariablesColumn 
    public constant text NAME = "LocalsName"
    public constant text TYPE = "LocalsType"
    public constant text VALUE = "LocalsValue"
    text current = NAME

    action GetCurrent returns text
        return current
    end

    action SetToName
        current = NAME
    end

    action SetToType
        current = TYPE
    end

    action SetToValue
        current = VALUE
    end
end